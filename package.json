{
  "name": "vue-phaser-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --host",
    "build": "vite build --mode production",
    "build:ssg": "vite-ssg build --mode production",
    "preview": "vite preview --host",
    "start": "pnpm build && pnpm preview --host",
    "start:ssg": "pnpm build:ssg && pnpm preview --host",
    "upload": "pnpm build && node deploy.mjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "phaser": "^3.87.0",
    "phaser-ez": "^0.0.0",
    "pinia": "^2.3.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "basic-ftp": "^5.0.5",
    "core-js": "^3.39.0",
    "eslint": "latest",
    "eslint-plugin-vue": "latest",
    "jsonfile": "^6.1.0",
    "kolorist": "^1.8.0",
    "prettier": "latest",
    "sass": "^1.82.0",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "terser": "^5.37.0",
    "vite": "^6.0.3",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vite-ssg": "^0.24.2"
  }
}
